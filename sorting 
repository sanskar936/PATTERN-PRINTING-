#include <stdio.h>

void bubbleSort(float a[], int n) {
    for (int i = 0; i < n - 1; i++)
        for (int j = 0; j < n - i - 1; j++)
            if (a[j] > a[j + 1]) {
                float temp = a[j];
                a[j] = a[j + 1];
                a[j + 1] = temp;
            }
}

void selectionSort(float a[], int n) {
    for (int i = 0; i < n - 1; i++) {
        int min = i;
        for (int j = i + 1; j < n; j++)
            if (a[j] < a[min])
                min = j;
        float temp = a[i];
        a[i] = a[min];
        a[min] = temp;
    }
}

void insertionSort(float a[], int n) {
    for (int i = 1; i < n; i++) {
        float key = a[i];
        int j = i - 1;
        while (j >= 0 && a[j] > key) {
            a[j + 1] = a[j];
            j--;
        }
        a[j + 1] = key;
    }
}

void display(float a[], int n) {
    for (int i = 0; i < n; i++)
        printf("%.2f  ", a[i]);
    printf("\n");
}

int main() {
    int n;
    printf("Enter number of products: ");
    scanf("%d", &n);
    float price[n];

    printf("Enter product prices:\n");
    for (int i = 0; i < n; i++)
        scanf("%f", &price[i]);

    printf("\nOriginal Prices:\n");
    display(price, n);

    float a1[n], a2[n], a3[n];
    for (int i = 0; i < n; i++) {
        a1[i] = a2[i] = a3[i] = price[i];
    }

    bubbleSort(a1, n);
    selectionSort(a2, n);
    insertionSort(a3, n);

    printf("\nSorted (Bubble Sort):\n");
    display(a1, n);

    printf("Sorted (Selection Sort):\n");
    display(a2, n);

    printf("Sorted (Insertion Sort):\n");
    display(a3, n);

    return 0;
}