#include <stdio.h>
#define SIZE 10

int main() {
    int q[SIZE], front = -1, rear = -1, ch, id;

    do {
        printf("\n1.Add Parcel\n2.Dispatch Parcel\n3.Display\n4.Exit\nEnter choice: ");
        scanf("%d", &ch);

        switch (ch) {
        case 1:
            if ((rear + 1) % SIZE == front)
                printf("Queue Full!\n");
            else {
                printf("Enter Parcel ID: ");
                scanf("%d", &id);
                if (front == -1) front = rear = 0;
                else rear = (rear + 1) % SIZE;
                q[rear] = id;
            }
            break;

        case 2:
            if (front == -1)
                printf("Queue Empty!\n");
            else {
                printf("Dispatched Parcel ID: %d\n", q[front]);
                if (front == rear) front = rear = -1;
                else front = (front + 1) % SIZE;
            }
            break;

        case 3:
            if (front == -1)
                printf("Queue Empty!\n");
            else {
                printf("Pending Parcels: ");
                for (int i = front; ; i = (i + 1) % SIZE) {
                    printf("%d ", q[i]);
                    if (i == rear) break;
                }
                printf("\n");
            }
            break;
        }
    } while (ch != 4);

    return 0;
}